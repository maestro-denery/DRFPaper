diff --git a/build.gradle.kts b/build.gradle.kts
index 01798255d45f2a642df00156f11dd2bcd8108079..37f9da59f991d76491d227e1bf3dc85d26de8df4 100644
--- a/build.gradle.kts
+++ b/build.gradle.kts
@@ -9,6 +9,7 @@ java {
 }
 
 val adventureVersion = "4.10.1"
+val drfVersion = "0.1.0" // DRFPaper - DRF version.
 val apiAndDocs: Configuration by configurations.creating {
     attributes {
         attribute(Category.CATEGORY_ATTRIBUTE, objects.named(Category.DOCUMENTATION))
@@ -39,6 +40,12 @@ dependencies {
     apiAndDocs("net.kyori:adventure-text-serializer-gson")
     apiAndDocs("net.kyori:adventure-text-serializer-legacy")
     apiAndDocs("net.kyori:adventure-text-serializer-plain")
+    // DRFPaper start
+    apiAndDocs(platform("net.drf:drf-bom:$drfVersion"))
+    apiAndDocs("net.drf:drf-base")
+    apiAndDocs("net.drf:drf-data-addons")
+    apiAndDocs("net.drf:drf-registries")
+    // DRFPaper end
     api("org.apache.logging.log4j:log4j-api:2.17.1")
     api("org.slf4j:slf4j-api:1.8.0-beta4")
 
